import java.util.Scanner;
class Main {
  public static void main(String[] args) {

    int fact=1, num, cont=1;
        Scanner Entrada = new Scanner(System.in);
        System.out.println("Ingrese un n√∫mero: ");
        num = Entrada.nextInt();
        do {
            fact = fact*cont;
            cont = cont+1;
        } while (cont<=num);
        System.out.println("El factorial es: "+fact);
        

    
  }
}
